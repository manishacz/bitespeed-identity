{
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./dist",
    "module": "commonjs",
    "target": "ES2019",
    "lib": ["esnext"],
    "sourceMap": true,
    "esModuleInterop": true,
    "strict": false,
    "isolatedModules": true,
    "moduleDetection": "force",
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "dist", "prisma.config.ts"]
}